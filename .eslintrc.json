{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "arrow-body-style": [
      "error",
      "always"
    ],
    "no-console": "off",
    "semi": "error",
    "camelcase": "off",
    "eol-last": "off",
    "consistent-return": "off",
    "quotes": "off",
    "import/prefer-default-export": "off",
    "indent": 4
  }
}